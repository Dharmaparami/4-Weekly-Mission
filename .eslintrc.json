{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["eslint:recommended", "eslint-config-prettier"],
  "plugins": ["eslint-plugin-prettier", "import"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "import/extensions": ["error", "ignorePackages"],
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto"
      }
    ]
  }
}
